name: download_data
conda.env: conda.yml

entry_points:
    main:
        parameters:
            file_url:
                description: URL of the file to download
                type: uri
            artifact_name:
                description: Name for the output artifact
                type: str
            artifact_type:
                description: Type for the output artifact
                type: str
                default: raw_data
            artifact_description:
                description: Description for the output artifact
                type:
        command: >-
            python download_data.py --file_url {file_url} --artifact_name {artifact_name} --artifact_type {artifact_type} --artifact_description {artifact_description}
